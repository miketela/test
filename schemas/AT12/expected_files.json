{
  "atom": "AT12",
  "description": "Expected files for AT12 regulatory atom",
  "subtypes": {
    "BASE_AT12": {
      "description": "Base AT12 data with loan and borrower information",
      "patterns": [
        "BASE_AT12_[YYYYMMDD].CSV",
        "BASE_AT12_[YYYYMMDD].XLSX"
      ],
      "pattern": "BASE_AT12_[YYYYMMDD].(CSV|XLSX)",
      "required": true,
      "frequency": "monthly",
      "supported_formats": ["csv", "xlsx"]
    },
    "TDC_AT12": {
      "description": "TDC AT12 data with credit card guarantees information",
